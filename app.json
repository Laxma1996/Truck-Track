{
  "expo": {
    "name": "Truck Tracker",
    "slug": "truck-tracker",
    "version": "1.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trucktracker.app",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to take photos of trucks.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select truck images.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.trucktracker.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "66e1c984-c6df-4d85-a032-feab6fca63d0"
      }
    },
    "owner": "lakshmakandula"
  }
}
